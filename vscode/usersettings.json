{
    "workbench.activityBar.visible": false,
    "editor.smoothScrolling": true,
    "editor.mouseWheelScrollSensitivity": 2,
    "merge-conflict.decorators.enabled": false,
    "window.zoomLevel": 0,
    "[html]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.detectIndentation": false
    },
    "explorer.confirmDragAndDrop": false,
    "emmet.extensionsPath": "",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.minimap.enabled": false,
    "window.title": "${dirty}${activeFolderMedium}${separator}${rootName}${separator}${appName}",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.colorCustomizations": {
        "tab.activeModifiedBorder": "#B90F0A",
        "tab.activeBorder": "#FF7417"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.labelFormat": "short",
    "breadcrumbs.enabled": true,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Adapta Nokto",
    "window.menuBarVisibility": "toggle"
}
